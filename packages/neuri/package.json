{
  "name": "neuri",
  "type": "module",
  "version": "0.1.5",
  "description": "Simple and easy agent framework, include various of structured data manipulation, agent and function compositing, code editing, fs and more!",
  "author": {
    "name": "Lingtic.io Neuri Team",
    "email": "neuridev@lingtic.io",
    "url": "https://github.com/lingticio"
  },
  "license": "MIT",
  "homepage": "https://docs.lingtic.io/pages/en/neuri",
  "repository": {
    "type": "git",
    "url": "https://github.com/lingticio/neuri-js.git",
    "directory": "packages/neuri"
  },
  "keywords": [
    "guiiai",
    "llm",
    "openai",
    "lingtic",
    "neuri"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./openai": {
      "types": "./dist/openai/index.d.ts",
      "import": "./dist/openai/index.mjs"
    },
    "./test": {
      "types": "./dist/test.d.ts",
      "import": "./dist/test.mjs"
    }
  },
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "stub": "unbuild --stub",
    "build": "unbuild",
    "package:publish": "pnpm publish --access public --no-git-checks",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@guiiai/logg": "^1.0.10",
    "@xsai/generate-text": "^0.2.1",
    "@xsai/shared": "0.2.0",
    "@xsai/shared-chat": "^0.2.1",
    "@xsai/stream-text": "^0.2.1",
    "@xsai/tool": "^0.2.1",
    "@xsai/utils-chat": "^0.2.1",
    "defu": "^6.1.4",
    "nanoid": "^5.1.5",
    "xsschema": "^0.2.1"
  },
  "devDependencies": {
    "@fetch-mock/vitest": "^0.2.13",
    "@types/mdast": "^4.0.4",
    "@types/node": "^22.15.17",
    "@xsai-ext/providers-cloud": "^0.2.1",
    "fetch-mock": "^12.5.2",
    "json-schema": "^0.4.0",
    "remark": "^15.0.1",
    "zod": "^3.24.4"
  }
}
